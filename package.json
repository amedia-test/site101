{
  "name": "site101",
  "version": "1.0.0",
  "private": true,
  "description": "site101 application",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": ""
  },
  "license": "UNLICENSED",
  "author": "Amedia",
  "type": "module",
  "main": "./build/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node ./build/server.js",
    "dev": "NODE_ENV=development node --watch --inspect ./build/server.js",
    "lint": "kragl lint",
    "lint:fix": "kragl lint --fix",
    "lint:format": "kragl format",
    "test:static": "npm run lint",
    "test:unit": "NODE_ENV=test vitest --exclude=src/**/*.integration.spec.js",
    "test:unit:watch": "NODE_ENV=test npm run test:unit -- --watch",
    "test:integration": "NODE_ENV=test vitest src/**/*.integration.spec.js",
    "test:integration:watch": "npm run test:integration -- --watch",
    "test": "npm run test:static && npm run test:unit && npm run test:integration"
  },
  "dependencies": {
    "@amedia/atomizer-header": "7.0.1",
    "@amedia/health-monitor": "3.1.0",
    "@amedia/metrics": "4.0.1",
    "@amedia/node-log": "4.0.1",
    "@amedia/site-config": "^4.0.1",
    "@fastify/cors": "11.1.0",
    "@fastify/etag": "6.1.0",
    "convict": "6.2.4",
    "fastify": "5.6.1"
  },
  "devDependencies": {
    "@amedia/eslint-config": "4.0.7",
    "@amedia/kragl": "5.1.1",
    "@amedia/prettier-config": "2.1.1",
    "@types/convict": "6.1.6",
    "@types/node": "24.10.0",
    "@vitest/coverage-v8": "4.0.7",
    "pino-pretty": "13.1.2",
    "typescript": "5.9.3",
    "vitest": "4.0.7"
  }
}
